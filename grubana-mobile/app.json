{
  "expo": {
    "name": "Grubana",
    "slug": "grubana",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "grubana",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#170B1F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.grubana.mobile",
  "buildNumber": "22",
      "requireFullScreen": false,
      "userInterfaceStyle": "automatic",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to show nearby food trucks and send location-based pings.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to show nearby food trucks and send location-based pings.",
        "NSCameraUsageDescription": "This app needs camera access to upload food truck photos.",
        "NSPhotoLibraryUsageDescription": "This app needs photo library access to upload food truck photos.",
        "ITSAppUsesNonExemptEncryption": false,
        "UIViewControllerBasedStatusBarAppearance": false,
        "UIStatusBarStyle": "UIStatusBarStyleLightContent"
      },
      "associatedDomains": [
        "applinks:grubana.com",
        "applinks:www.grubana.com"
      ]
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.grubana.mobile",
      "adaptiveIcon": {
        "foregroundImage": "./assets/foreground.png",
        "backgroundImage": "./assets/background.png"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "FOREGROUND_SERVICE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-location",
      "expo-font"
    ],
    "submit": {
      "production": {
        "ios": {
          "appleId": "6752311735",
          "ascAppId": "6752311735",
          "appleTeamId": "2NW6YF9UXT"
        }
      }
    },
    "extra": {
      "eas": {
        "projectId": "6d8ae2b7-1cc1-4556-8dcc-7a536106989d"
      }
    },
    "owner": "stellarmerit"
  }
}
