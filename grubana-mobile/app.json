{
  "expo": {
    "name": "Grubana",
    "slug": "grubana",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "grubana",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2c6f57"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.grubana.mobile",
      "buildNumber": "6",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to show nearby food trucks and send location-based pings.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to show nearby food trucks and send location-based pings.",
        "NSCameraUsageDescription": "This app needs camera access to upload food truck photos.",
        "NSPhotoLibraryUsageDescription": "This app needs photo library access to upload food truck photos.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:grubana.com",
        "applinks:www.grubana.com"
      ]
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.grubana.mobile",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "FOREGROUND_SERVICE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-location",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "6d8ae2b7-1cc1-4556-8dcc-7a536106989d"
      }
    },
    "owner": "stellarmerit"
  }
}
